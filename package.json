{
  "name": "web-ckgen-prmon",
  "version": "0.0.0-1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href /ckgen/dcalc/",
    "build:release": "./node_modules/.bin/ng build --prod --base-href /ckgen/sdc/ --progress --output-path=dist/ckgen/sdc",
    "test": "jest --no-cache",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.1.3",
    "@angular/cdk": "13.1.2",
    "@angular/cli": "13.1.3",
    "@angular/common": "13.1.3",
    "@angular/compiler": "13.1.3",
    "@angular/core": "13.1.3",
    "@angular/forms": "13.1.3",
    "@angular/material": "13.1.2",
    "@angular/platform-browser": "13.1.3",
    "@angular/platform-browser-dynamic": "13.1.3",
    "@angular/router": "^13.2.3",
    "@fortawesome/angular-fontawesome": "0.9.0",
    "@fortawesome/fontawesome-free": "6.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-regular-svg-icons": "5.15.3",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@microsoft/signalr": "6.0.2",
    "@monitel/ckgen-wrapper-lib": "^2.0.0",
    "@monitel/ng-token": "^4.0.0",
    "@types/file-saver": "2.0.5",
    "angular-split": "5.0.0",
    "devextreme": "21.1.4",
    "devextreme-angular": "21.1.4",
    "file-saver": "2.0.5",
    "moment": "2.29.1",
    "rxjs": "6.6.7",
    "tslib": "2.1.0",
    "uuid": "^8.3.2",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.1.3",
    "@angular/compiler-cli": "13.1.3",
    "@types/file-saver": "2.0.5",
    "@types/node": "12.11.1",
    "jasmine": "^2.8.0",
    "jest": "^27.5.1",
    "protractor": "^7.0.0",
    "typescript": "4.5.5"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
