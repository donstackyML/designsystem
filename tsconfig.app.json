{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@sdc/*": ["./src/app/sdc-log/*"],
      "@dcalc/*": ["./src/app/dcalc/*"],
      "@app/*": ["./src/app/*"],
      "jszip": ["node_modules/jszip/dist/jszip.min.js"]
    }
  },
  "files": ["src/main.ts", "src/polyfills.ts"],
  "include": ["src/**/*.d.ts"]
}
